
	
	<%= simple_form_for @pic do |f| %>

		<%= f.input :lesson %>
		
		<br/>
		<%= f.input :image, label: "Enter a New Lesson" %>
		<br/>

		<%= f.input :emotion, label: 'How did you feel?', placeholder: 'Stoned, Jazzed, Super, etc'%>
		<%= f.input :learned, label: 'What did you learn?' , placeholder: 'Ruby'%>
		
		<%= submit_tag 'Enter Data', :class =>'btn btn-success btn-large' %> 

	<% end %>




