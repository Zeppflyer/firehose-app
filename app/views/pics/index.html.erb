

	<% @pics.each do|pic| %>
		
		<div class="stolen-frosty span6 text-center">
		
		<h1 class="stolen-h1">
		Lesson: <%= pic.lesson %>
		</h1>
		<br/>

			<%= link_to pic_path(pic) do%>
			
			<%= image_tag pic.image%>
			<% end %>

		<h2>
		Emotion: <%= pic.emotion %>
		<br/>
		<br/>

		Learned: <%= pic.learned %>
		</h2>

		<br/>
	</div>

	<% end %>
